/**
 * 重置所有下拉菜单
 */
function resetAllDrawDownList(currentList)
{
    if(currentList != "homePageDrawDownList"){
        $("#homePageDrawDownList").hide();
    }
    if(currentList != "newTabAtDrawDownList"){
        $("#newTabAtDrawDownList").hide();
    }
	if(currentList != "downloadAtDrawDownList"){
        $("#downloadAtDrawDownList").hide();
    }
    if(currentList != "tabClosePolicyDrawDownList"){
        $("#tabClosePolicyDrawDownList").hide();
    }
    if(currentList != "cleanPrivacyTimeDrawDownList"){
        $("#cleanPrivacyTimeDrawDownList").hide();
    }
	if(currentList != "bossKeyDrawDownList"){
        $("#bossKeyDrawDownList").hide();
    }
	if(currentList != "languageDrawDownList"){
        $("#languageDrawDownList").hide();
    }
	if(currentList != "shoppingHelperDrawDownList"){
        $("#shoppingHelperDrawDownList").hide();
    }
	if(currentList != "translateHelperDrawDownList"){
        $("#translateHelperDrawDownList").hide();
    }
}
