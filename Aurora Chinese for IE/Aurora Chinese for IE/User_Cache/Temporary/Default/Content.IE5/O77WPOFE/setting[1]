/**
 * 置项主流程
 */
$(document).ready(function(){
		
    /**
     * 设置接口对象
     */
    /**
     * 初始化设置选项
     */
	var settingObj = new setting();

	initSetting(settingObj);

    /**
     * 点击导航单选按钮
     */
    $("#hao123CheckRadio").parent("li").click(function(){
        if($("#hao123CheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setStartPageType("0");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击上次关闭时浏览的网页单选按钮
     */
    $("#lastCloseCheckRadio").parent("li").click(function(){
        if($("#lastCloseCheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setStartPageType("1");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击主页单选按钮
     */
    $("#homePageCheckRadio").parent("li").click(function(){
        if($("#homePageCheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setStartPageType("2");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击主页单下拉列表
     */
    $("#homePageDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("homePageDrawDownList");
        $("#homePageDrawDownList").toggle();
    });

    /**
     * 选择主页下拉列表选项
     */
    $("#homePageDrawDownList").delegate("li","click",function(){
        var optionID = $(this).attr("id");
        if(optionID == "hao123Option"){
            settingObj.setStartPageType("2");
            settingObj.setCurSelHomePage("0");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
        if(optionID == "baiduOption"){
            settingObj.setStartPageType("2");
            settingObj.setCurSelHomePage("1");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
        if(optionID == "newPageOption"){
            settingObj.setStartPageType("2");
            settingObj.setCurSelHomePage("2");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
        if(optionID == "homePageUserDefinedOption"){
            settingObj.setStartPageType("2");
            settingObj.setCurSelHomePage("3");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
		if(optionID == "lastOpenPageOption"){
            settingObj.setStartPageType("2");
            settingObj.setCurSelHomePage("4");
            resetStartPageType(settingObj);
            showSuccessMessage();
        }
		if(optionID == "userDefinedOption"){
            showUserDefinedHomePageForm(settingObj);
        }
    });
	
	/**
	 *下拉选单滑过效果合集
	 */

	//左侧选单
	$(".leftList ul").on("mouseenter", 'li', function(){
		if($(this).hasClass("current")){
			$(this).attr("class", "current");
		}else{
			$(this).attr("class", "leftLiHov");
			}
    	});
    $(".leftList ul").on("mouseleave", 'li', function(){
        if($(this).hasClass("current")){
			 $(this).attr("class", "current");
			}
		if($(this).hasClass("leftLiHov")){
			 $(this).attr("class", "");
			}
    	});

	//首页选择
	$("#homePageDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#homePageDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//老板键
	$("#bossKeyDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#bossKeyDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//语言选择
	$("#languageDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#languageDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//新标签页	
	$("#newTabAtDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#newTabAtDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//标签页关闭
	$("#tabClosePolicyDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#tabClosePolicyDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//下载选项	
	$("#downloadAtDrawDownList").on("mouseenter", 'li', function(){
		if($(this).hasClass("fontGray")){
			return;
			}else{
				$(this).attr("class", "liHov");
			}
    	});
    $("#downloadAtDrawDownList").on("mouseleave", 'li', function(){
		if($(this).hasClass("fontGray")){
			return;
			}else{
				$(this).attr("class", "");
			}
    	});
	//清除缓存
	$("#cleanPrivacyTimeDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#cleanPrivacyTimeDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});
	//购物助手
	$("#shoppingHelperDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#shoppingHelperDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});  
	//翻译选择
	$("#translateHelperDrawDownList").on("mouseenter", 'li', function(){
		$(this).attr("class", "liHov");
    	});
    $("#translateHelperDrawDownList").on("mouseleave", 'li', function(){
        $(this).attr("class", "");
    	});  
    /**
     * 老板键下拉列表选项
     */
    $("#bossKeyDrawDownList").delegate("li","click",function(){
		var bossKey = settingObj.getBossKey();
        var data = $(this).children("span").attr("data");
        settingObj.setBossKey(data);
		checkBossKeyCustomStatus(settingObj,bossKey);
    });
	
	/**
	* 老板键鼠标中键
	*/

    $("#bossKeyMidKey").parent("li").click(function(){
        var value = settingObj.getBossKeyMidKey();
        if(value == 1){
            settingObj.setBossKeyMidKey("0");
        }else{
            settingObj.setBossKeyMidKey("1");
        }
        resetBossKeyMidKey(settingObj);
        showSuccessMessage();
    });
	/**
     * 语言选择下拉列表选项
     */
	var cl = settingObj.getProcessLanguage();
    $("#languageDrawDownList").delegate("li","click",function(){
		var lan = $(this).children("span").attr("lang");
        changeLanguage(settingObj)
		if(lan == cl){
			$("#languageTip").hide();
		}else{
			$("#languageTip").show();
			showSuccessMessage();
		}
		
    });
    /**
     * 点击用户自定义首页关闭按钮
     */
    $("#userDefinedHomePageFormClose").click(function(){
        hideUserDefinedHomePageForm();
    });

    /**
     * 点击用户自定义首页完成按钮
     */
    $("#userDefinedHomePageFormConfirm").click(function(){
        var value = $("#userDefinedHomePageFormText").text();
		value = value.replace(/\r/g,"\r\n");
		settingObj.setStartPageType("2");
        settingObj.setCurSelHomePage("3");
        settingObj.setHomePage(value);
        resetStartPageType(settingObj);
        hideUserDefinedHomePageForm();
        showSuccessMessage();
    });

    /**
     * 点击用户自定义首页取消按钮
     */
    $("#userDefinedHomePageFormCancel").click(function(){
        hideUserDefinedHomePageForm();
    });

    /**
     * 默认浏览器
     */

    /**
     * 点击设为默认浏览器按钮
     */
    $("#defaultBrow").click(function(){
        if($("#defaultBrowOff").is(":hidden")){
            settingObj.setDefBrowser();
            clearInterval(checkDefBrowserTimer);
            checkDefBrowserTimer = setInterval(function(){
                checkDefBrowser(settingObj);
            },500);
        }
    });

    /**
     * 广告过滤
     */

    /**
     * 点击不过滤广告单选按钮
     */
    $("#noADFilterCheckRadio").parent("li").click(function(){
        if($("#noADFilterCheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setADFilterClass("0");
            resetADFilterClass(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击仅拦截自动弹出广告单选按钮
     */
    $("#aotoADFilterCheckRadio").parent("li").click(function(){
        if($("#aotoADFilterCheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setADFilterClass("1");
            resetADFilterClass(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击拦截自动弹出的网页和浮动广告单选按钮
     */
    $("#aotoAndFloatADFilterCheckRadio").parent("li").click(function(){
        if($("#aotoAndFloatADFilterCheckRadio").attr("class") != "checkRadio_act"){
            settingObj.setADFilterClass("2");
            resetADFilterClass(settingObj);
            showSuccessMessage();
        }
    });

    /**
     * 点击拦截自动弹出广告复选按钮
     */
    $("#winBlockCheckBox").parent("li").click(function(){
        if($("#winBlockCheckBox").attr("class") != "checkBox_act"){
            settingObj.setWinBlock("1");
        }else{
            settingObj.setWinBlock("0");
        }
        resetWinBlock(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击拦截页面广告复选按钮
     */
    $("#adBlockCheckBox").parent("li").click(function(){
        if($("#adBlockCheckBox").attr("class") != "checkBox_act"){
            settingObj.setAdBlock("1");
        }else{
            settingObj.setAdBlock("0");
        }
        resetAdBlock(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击管理不过滤的网站按钮
     */
    $("#manageNoADFilterBtn").click(function(){
        showManageNoADFilterForm(settingObj);
    });

    /**
     * 点击管理不过滤的网站窗口确定按钮
     */
    $("#manageNoADFilterConfirmBtn").click(function(){
        hideManageNoADFilterForm();
    });

    /**
     * 点击管理不过滤的网站窗口取消按钮
     */
    $("#manageNoADFilterCancleBtn").click(function(){
        hideManageNoADFilterForm();
    });

    /**
     * 点击管理不过滤的网站窗口关闭按钮
     */
    $("#manageNoADFilterCloseBtn").click(function(){
        hideManageNoADFilterForm();
    });

    /**
     * 点击删除不过滤的网站按钮
     */
    $('#manageNoADFilterList').on('click', '.delNoADFilterItem', function(){
		var site = $(this).parent('span').parent('li').children('h4').html();
       // alert(site);
		settingObj.delWinBlockItem(site);
        $(this).parent('span').parent('li').hide();
    });
	
	/**
     * 点击保存密码复选按钮
     */
    $("#passwordSaveCheckBox").parent("p").click(function(){
        if($("#passwordSaveCheckBox").attr("class") != "checkBox_act"){
			settingObj.setLoginAssis("1");
        }else{
            settingObj.setLoginAssis("0");
        }
        resetPasswordSave(settingObj)
        showSuccessMessage();
    });
	/**
	 * 点击已保存密码网站管理窗口
	 */
    $("#managePasswordSaveBtn").click(function(){
		resetPasswordSaveFormHeight();
        showManagePasswordSaveForm(settingObj);
    });
	/**
	 * 点击已保存密码网站管理窗口
	 */
    $("#managePasswordNoSaveBtn").click(function(){
		resetPasswordNoSaveFormHeight();
        showManagePasswordNoSaveForm(settingObj);
    });
	
    /**
     * 点击buscape shopping helper checkbox
     */
    $("#shoppingHelperCheckBox").parent("p").click(function(){
        if($("#shoppingHelperCheckBox").attr("class") != "checkBox_act"){
            settingObj.setShopHelperSetting("1");
        }else{
            settingObj.setShopHelperSetting("0");
        }
        resetShoppingHelper(settingObj)
        showSuccessMessage();
    });
	/**
     * 点击aurora translate helper checkbox
     */
    $("#translateHelperCheckBox").parent("p").click(function(){
        if($("#translateHelperCheckBox").attr("class") != "checkBox_act"){
            settingObj.setTranslateHelperMode("1");
        }else{
            settingObj.setTranslateHelperMode("0");
        }
        resetTranslateHelper(settingObj)
        showSuccessMessage();
    });
    
    /**
     * 关闭浏览器时
     */

    /**
     * 点击若有多个标签则询问是否要关闭浏览器复选按钮
     */
    $("#multiTabAskCloseCheckBox").parent("li").click(function(){
        var value = settingObj.getCloseMultiTabTip();
        if(value == 1){
            settingObj.setCloseMultiTabTip("0");
        }else{
            settingObj.setCloseMultiTabTip("1");
        }
        resetConfirmCloseAll(settingObj);
        showSuccessMessage();
    });
	
    /**
     * 点击选用桔子下载时保存复选按钮
     */
    $("#askDownloadSaveLocation").parent("li").click(function(){
        var value = settingObj.getAskSaveLocation();
        if(value == 1){
            value == 0
	    settingObj.setAskSaveLocation("0");
        }else{
		value == 1
		settingObj.setAskSaveLocation("1");
        }
        resetDownloadSaveLocation(settingObj);
        showSuccessMessage();
    });
	
	/**
     * 点击选用桔子下载时显示列表复选按钮
     */
    $("#showDownloadListWindow").parent("li").click(function(){
        var value = settingObj.getShowDownlistWnd();
        if(value == 1){
            value == 0
	    	settingObj.setShowDownlistWnd("0");
        }else{
			value == 1
 	        settingObj.setShowDownlistWnd("1");
        }
        resetDownloadListWindow(settingObj);
        showSuccessMessage();
    });
	
	
	/**
     * 点击老板键下拉列表
     */
    $("#bossKeyDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("bossKeyDrawDownList");
        $("#bossKeyDrawDownList").toggle();
    });
	
	/**
     * 点击语言选择下拉列表
     */
    $("#languageDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("languageDrawDownList");
        $("#languageDrawDownList").toggle();
    });
	
    /**
     * 鼠标手势
     */

    /**
     * 使用鼠标手势
     */

    /**
     * 点击开启鼠标手势复选按钮
     */
    $("#mouseGestureCheckBox").parent("p").click(function(){
        var value = settingObj.getMouseGesture();
        if(value == 1){
            settingObj.setMouseGesture("0");
        }else{
            settingObj.setMouseGesture("1");
        }
        resetMouseGesture(settingObj);
        showSuccessMessage();
    });

    /**
     * 左侧菜单
     */

    /**
     * 点击基本设置菜单
     */
    $("#baseSettingMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("baseSettingMenu");
        setCurrentSettingMain("baseSetting");
        resetRightOptionSize();
		MouseGestureListClean();
    });

    /**
     * 点击上网痕迹菜单
     */
    $("#internetTracesMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("internetTracesMenu");
        setCurrentSettingMain("internetTraces");
        resetRightOptionSize();
		 MouseGestureListClean();
    });

    /**
     * 点击标签设置菜单
     */
    $("#tabSettingMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("tabSettingMenu");
        setCurrentSettingMain("tabSetting");
        resetRightOptionSize();
		MouseGestureListClean();
    });
	
	 /**
     * 点击下载设置菜单
     */
    $("#downloadSettingMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("downloadSettingMenu");
        setCurrentSettingMain("downloadSetting");
        resetRightOptionSize();
		MouseGestureListClean();
    });

    /**
     * 点击鼠标手势菜单
     */
    $("#mouseHandMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("mouseHandMenu");
        setCurrentSettingMain("mouseHand");
        resetRightOptionSize();
		intiMouseGestureNum(settingObj);
    });
	
    /**
     * 点击更多设置菜单
     */
    $("#moreMenu").click(function(){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("moreMenu");
        setCurrentSettingMain("moreSetting");
        resetRightOptionSize();
		MouseGestureListClean();
	//intiMouseGestureNum(settingObj);
    });

    /**
     * 上网痕迹
     */

    /**
     * 清除上网痕迹
     */

    /**
     * 点击清除网页浏览记录复选按钮
     */
    $("#cleanPrivacyCheckBox2").parent("li").click(function(){
        var classname = $("#cleanPrivacyCheckBox2").attr("class");
        if(classname == "checkBox_act"){
            resetCleanPrivacyCheckBox("2");
        }else{
            setCleanPrivacyCheckBox("2");
        }
        setCleanPrivacyOption(settingObj);
        resetCleanPrivacyOption(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击清除保存的网页表单数据复选按钮
     */
    $("#cleanPrivacyCheckBox16").parent("li").click(function(){
        var classname = $("#cleanPrivacyCheckBox16").attr("class");
        if(classname == "checkBox_act"){
            resetCleanPrivacyCheckBox("16");
        }else{
            setCleanPrivacyCheckBox("16");
        }
        setCleanPrivacyOption(settingObj);
        resetCleanPrivacyOption(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击清除地址栏下拉列表复选按钮
     */
    $("#cleanPrivacyCheckBox8").parent("li").click(function(){
        var classname = $("#cleanPrivacyCheckBox8").attr("class");
        if(classname == "checkBox_act"){
            resetCleanPrivacyCheckBox("8");
        }else{
            setCleanPrivacyCheckBox("8");
        }
        setCleanPrivacyOption(settingObj);
        resetCleanPrivacyOption(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击清除缓存的临时文件复选按钮
     */
    $("#cleanPrivacyCheckBox1").parent("li").click(function(){
        var classname = $("#cleanPrivacyCheckBox1").attr("class");
        if(classname == "checkBox_act"){
            resetCleanPrivacyCheckBox("1");
        }else{
            setCleanPrivacyCheckBox("1");
        }
        setCleanPrivacyOption(settingObj);
        resetCleanPrivacyOption(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击清除Cookie等网站数据复选按钮
     */
    $("#cleanPrivacyCheckBox4").parent("li").click(function(){
        var classname = $("#cleanPrivacyCheckBox4").attr("class");
        if(classname == "checkBox_act"){
            resetCleanPrivacyCheckBox("4");
        }else{
            setCleanPrivacyCheckBox("4");
        }
        setCleanPrivacyOption(settingObj);
        resetCleanPrivacyOption(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击开始清除痕迹按钮
     */
    $("#startCleanPrivacyBtn").click(function(){
        var value = settingObj.getCleanPrivacyStatus();
        if(value == "0"){
            startCleanPrivacy(settingObj);
            checkCleanPrivacyStatus(settingObj);
            showCleanningMessage();
            $(".savedTip").attr("flag", "1");
			$("#startCleanPrivacyBtn").hide();
   			$("#startCleanPrivacyBtnOff").show();
        }else{
			$("#startCleanPrivacyBtn").show();
   			$("#startCleanPrivacyBtnOff").hide();

			//清除痕迹按钮状态
			$("#startCleanPrivacyBtn").on("mouseenter",function(){
				$(this).attr("class", "btnSetDefaultHov");
				});
			$("#startCleanPrivacyBtn").on("mouseleave",function(){
				$(this).attr("class", "btnSetDefault");
				});
			$("#startCleanPrivacyBtn").on("mousedown",function(){
				$(this).attr("class", "btnSetDefaultAct");
				});
			$("#startCleanPrivacyBtn").on("mouseup",function(){
				$(this).attr("class", "checkDefBrowser");
				});				
			}
    });

    /**
     * 点击清除指定时间内的数据下拉框
     */
    $("#cleanPrivacyTimeDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("cleanPrivacyTimeDrawDownList");
        $("#cleanPrivacyTimeDrawDownList").toggle();
    });

    /**
     * 选择清除指定时间内的数据下拉列表
     */
    $("#cleanPrivacyTimeDrawDownList").delegate("li","click",function(){
        var text = $(this).children("span").html();
        $("#cleanPrivacyTimeText").html(text);
        setCleanPrivacyTime(settingObj);
        resetCleanPrivacyTime(settingObj);
        showSuccessMessage();
    });
	
    /**
     * 点击购物助手下拉框
     */
    $("#shoppingHelperDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("shoppingHelperDrawDownList");
        $("#shoppingHelperDrawDownList").toggle();
    });

    /**
     * 选择购物助手的数据下拉列表
     */
    $("#shoppingHelperDrawDownList").delegate("li","click",function(){
        var text = $(this).children("span").html();
        $("#shoppingHelperText").html(text);
    });
    /**
     * 自动清除
     */

    /**
     * 点击退出浏览器时自动清除上面勾选的数据复选按钮
     */
    $("#cleanPrivacyOnExitCheckBox").parent("li").click(function(){
        var value = settingObj.getCleanPrivacyOnExit();
        if(value == "1"){
            settingObj.setCleanPrivacyOnExit("0");
        }else{
            settingObj.setCleanPrivacyOnExit("1");
        }
        resetCleanPrivacyOnExit(settingObj);
        showSuccessMessage();
    });

    /**
     * 标签设置
     */

    /**
     * 新建标签
     */

    /**
     * 点击在新标签打开地址栏输入的网址复选按钮
     */
    $("#newTabFromAddrBoxCheckBox").parent("li").click(function(){
        var value = settingObj.getNewTabFromAddrBox();
        if(value == "1"){
            settingObj.setNewTabFromAddrBox("0");
        }else{
            settingObj.setNewTabFromAddrBox("1");
        }
        resetNewTabFromAddr(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击在新标签打开收藏夹里的网址复选按钮
     */
    $("#newTabFromBookmarkCheckBox").parent("li").click(function(){
        var value = settingObj.getNewTabFromBookmark();
        if(value == "1"){
            settingObj.setNewTabFromBookmark("0");
        }else{
            settingObj.setNewTabFromBookmark("1");
        }
        resetNewTabFromBookmark(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击点击链接新打开标签时，自动切换到该标签复选按钮
     */
    $("#alwaysActiveNewTabCheckBox").parent("li").click(function(){
        var value = settingObj.getAlwaysActiveNewTab();
        if(value == "1"){
            settingObj.setAlwaysActiveNewTab("0");
        }else{
            settingObj.setAlwaysActiveNewTab("1");
        }
        resetAlwaysActiveNewTab(settingObj);
        //showSuccessMessage();
    });

    /**
     * 点击新打开的标签放在下拉框
     */
    $("#newTabAtDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("newTabAtDrawDownList");
        $("#newTabAtDrawDownList").toggle();
    });

    /**
     * 选择新打开的标签放在下拉列表选项
     */
    $("#newTabAtDrawDownList").delegate("li","click",function(){
        var optionID = $(this).attr("id");
        if(optionID == "currentTabR"){
            settingObj.setNewTabAlignment("-1");
            resetNewTabAlignment(settingObj);
            showSuccessMessage();
        }
        if(optionID == "AllTabR"){
            settingObj.setNewTabAlignment("-3");
            resetNewTabAlignment(settingObj);
            showSuccessMessage();
        }
    });
	
	/**
     * 点击下载设置标签放在下拉框
     */
    $("#downloadAtDrawDown").click(function(event){
        event.stopPropagation();
		var lang = settingObj.getProcessLanguage();
			//if(cl == 1 || lang == 3){
				$("#chooseXunlei").hide();
				$("#chooseXunleiOff").hide();
				$("#chooseKuaiche").hide();
				$("#chooseKuaicheOff").hide();
				$("#chooseXuanfeng").hide();
				$("#chooseXuanfengOff").hide();
			//}
		
        resetAllDrawDownList("downloadAtDrawDownList");
        $("#downloadAtDrawDownList").toggle();
    });

    
	
	/**
     * 选择下载设置标签放在下拉列表选项
     */
    $("#newTabAtDrawDownList").delegate("li","click",function(){

    });
	

    /**
     * 关闭标签
     */

    /**
     * 点击双击关闭对应标签复选按钮
     */
    $("#dbClickCloseTabCheckBox").parent("li").click(function(){
        var value = settingObj.getDbClickCloseTab();
        if(value == "1"){
            settingObj.setDbClickCloseTab("0");
        }else{
            settingObj.setDbClickCloseTab("1");
        }
        resetDbClickCloseTab(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击右击关闭对应标签（按住shift）再右击可显示菜单复选按钮
     */
    $("#rbClickCloseTabCheckBox").parent("li").click(function(){
        var value = settingObj.getRbClickCloseTab();
        if(value == "1"){
            settingObj.setRbClickCloseTab("0");
        }else{
            settingObj.setRbClickCloseTab("1");
        }
        resetRbClickCloseTab(settingObj);
        showSuccessMessage();
    });

    /**
     * 点击关闭当前标签后，切换到下拉框
     */
    $("#tabClosePolicyDrawDown").click(function(event){
        event.stopPropagation();
        resetAllDrawDownList("tabClosePolicyDrawDownList");
        $("#tabClosePolicyDrawDownList").toggle();
    });

    /**
     * 选择关闭当前标签后，切换到下拉列表选项
     */
    $("#tabClosePolicyDrawDownList").delegate("li","click",function(){
        var optionID = $(this).attr("id");
        if(optionID == "tabClosePolicyS"){
            settingObj.setTabClosePolicy("0");
            resetTabClosePolicy(settingObj);
            showSuccessMessage();
        }
        if(optionID == "tabClosePolicyL"){
            settingObj.setTabClosePolicy("1");
            resetTabClosePolicy(settingObj);
            showSuccessMessage();
        }
        if(optionID == "tabClosePolicyR"){
            settingObj.setTabClosePolicy("2");
            resetTabClosePolicy(settingObj);
            showSuccessMessage();
        }
    });
	
	/**
     * 选择下载工具列表选项
     */
    $("#downloadAtDrawDownList").delegate("li","click",function(){
																
        var optionID = $(this).attr("id");
        if(optionID == "chooseEverTime"){
            settingObj.setDownloadPolicy("0");
            resetDownloadPolicy(settingObj);
            showSuccessMessage();
        }
        if(optionID == "chooseAurora"){
            settingObj.setDownloadPolicy("16");
            resetDownloadPolicy(settingObj);
            showSuccessMessage();
        }
        if(optionID == "chooseXunlei"){
			settingObj.setDownloadPolicy("17");
			resetDownloadPolicy(settingObj);
			showSuccessMessage();
        }
		if(optionID == "chooseKuaiche"){
			settingObj.setDownloadPolicy("18");
			resetDownloadPolicy(settingObj);
			showSuccessMessage();
        }
		if(optionID == "chooseXuanfeng"){
			settingObj.setDownloadPolicy("19");
			resetDownloadPolicy(settingObj);
			showSuccessMessage();
        }
    });
	/**
     * 选择购物助手选项
     */
    $("#shoppingHelperDrawDownList").delegate("li","click",function(){
        var optionID = $(this).attr("id");
        if(optionID == "showAll"){
			settingObj.setGWDSetting("0");
			showSuccessMessage();
        }
        if(optionID == "showPrice"){
 			settingObj.setGWDSetting("1");
           	showSuccessMessage();
        }
        if(optionID == "showTran"){
			settingObj.setGWDSetting("2");
			showSuccessMessage();
        }
		if(optionID == "showOff"){
			settingObj.setGWDSetting("3");
			showSuccessMessage();
        }
    });    
