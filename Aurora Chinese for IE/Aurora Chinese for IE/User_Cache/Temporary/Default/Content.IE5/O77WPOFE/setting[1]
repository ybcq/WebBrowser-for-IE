/**
 * 重置左侧菜单
 */
function resetLeftMenu()
{
	$("#baseSettingMenu").removeClass('current');
	$("#tabSettingMenu").removeClass('current');
	$("#downloadSettingMenu").removeClass('current');
	$("#internetTracesMenu").removeClass('current');
	$("#mouseHandMenu").removeClass('current');
	$("#moreMenu").removeClass('current');
}

/**
 * 设置当前菜单
 */
function setCurrentMenu(classname)
{
    $("#" + classname).addClass('current');
}

/**
 * 重置设置主界面
 */
function resetSettingMain()
{
	resetAllDrawDownList();
	$("#baseSetting").hide();
	$("#tabSetting").hide();
	$("#downloadSetting").hide();
	$("#internetTraces").hide();
	$("#mouseHand").hide();
	$("#moreSetting").hide();
	
	
}

/**
 * 设置设置主界面
 */
function setCurrentSettingMain(classname)
{
    $("#" + classname).show();
    resetLeftMenuHeight();
}

