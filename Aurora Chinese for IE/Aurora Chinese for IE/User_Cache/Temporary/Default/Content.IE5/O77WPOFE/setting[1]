function bossKeyMidKeyTipHide(){
		$("#bossKeyMidKeyOptionOn").hide();
		$("#bossKeyMidKeyOptionOff").show();
	}
function bossKeyMidKeyTipShow(){
		$("#bossKeyMidKeyOptionOn").show();
		$("#bossKeyMidKeyOptionOff").hide();
	}
function bossKeyMidKeyTipOn(){
		$("#bossKeyTip").show();
	}
function bossKeyMidKeyTipOff(){
		$("#bossKeyTip").hide();
	}
		
	/**
	*老板键鼠标中键开起关闭
	*/	
function resetBossKeyMidKey(settingObj)
	{	
		
		var value = settingObj.getBossKeyMidKey();
		if(value == 1){
			resetBossKeyMidKeyAct();
			bossKeyMidKeyTipOn()
		}else{
			resetBossKeyMidKeyReset();
			bossKeyMidKeyTipOff();
		}
	}
	
/**
 * 老板键鼠标中键按钮状态
 */
function resetBossKeyMidKeyAct()
{
    $("#bossKeyMidKey").removeClass('checkBox');
    $("#bossKeyMidKey").addClass('checkBox_act');
}

function resetBossKeyMidKeyReset()
{
    $("#bossKeyMidKey").removeClass('checkBox_act');
    $("#bossKeyMidKey").addClass('checkBox');
}

/**
 * 上次打开网页

function setExtGetRestore(newtabObj)
	{	

		var data = newtabObj.ExtGetRestore();
		
		var items = jsonObj.items;
		var editList2 = "";
		for(var i = 0; i < items.length; i++){
			if(items[i].url.indexOf("http:\/\/") >= 0 || items[i].url.indexOf("https:\/\/") >= 0){
				var icon = newtabObj.getUrlIconPath(items[i].url);

				editList2 = editList2 + '<li class="advEditLi"><h4><img src="' + icon + '"  /></h4><h5><span class="titleText">' + filterStr(items[i].title) + '</span>&emsp;<span class="urlText">' + filterUrl(items[i].url) + '</span></h5></li>';
			}
		}
  		$("#advEditList2 ul").html(editList2);	
	}
 */
 
/**
 * 过滤角号
 */
function filterStr(text)
{
	var div = document.createElement('div');
	div.innerText = text;
	return div.innerHTML;
}
/**
 * 过滤特殊字符
 */
	function filterUrl(url)
	{	
		if( url.indexOf("javascript:") == 0 ||  url.indexOf("vbscript:") == 0 ||  url.indexOf("jscript:") == 0 )
			return "";
		url = url.replace(/\\/g,"%5C");
		url = url.replace(/</g,"%3C");
		url = url.replace(/>/g,"%3E");
		url = url.replace(/"/g,"%22");
		url = url.replace(/'/g,"%27");
		url = url.replace(/`/g,"%60");
	
		return url;
	}


