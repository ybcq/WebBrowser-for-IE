/**
 * 显示用户自定义首页窗口
 */
function showUserDefinedHomePageForm(settingObj)
{
    var text = settingObj.getHomePage();
	text = text.replace(/\r\n/g,"\r");
    $("#userDefinedHomePageFormText").text(text);
    $("#userDefinedHomePageForm").show();
    resetUserDefinedHomePageFormHeight();
}

/**
 * 关闭用户自定义首页窗口
 */
function hideUserDefinedHomePageForm()
{
    $("#userDefinedHomePageForm").hide();
    $('html').css('overflow-y', 'hidden');
}

/**
 * 添加不过滤网站列表
 */
function addNoADFilterList(settingObj)
{
    var html = '';
    var data = settingObj.getWinBlockList();
    if(typeof(data) != "undefined"){
        var winBlockList = data.split('\r\n');
        for(var i = 0; i < winBlockList.length; i++){
            if(winBlockList[i] != ""){
                html += '<li><h4>' + winBlockList[i] + '</h4><span ><a href="#" class="delNoADFilterItem"><img src="close2.png" /></a></span></li>';
            }
        }
    }
    $("#manageNoADFilterList").html(html);
}

/**
 * 显示不过滤网站管理窗口
 */
function showManageNoADFilterForm(settingObj)
{
    addNoADFilterList(settingObj);
    $("#manageNoADFilterForm").show();
    resetManageNoADFilterFormHeight();
}

/**
 * 关闭不过滤网站管理窗口
 */
function hideManageNoADFilterForm()
{
    $("#manageNoADFilterForm").hide();
    $('html').css('overflow-y', 'hidden');
}

/**
 * 重置不过滤网站管理窗口高度
 */
function resetManageNoADFilterFormHeight()
{
    if($('#manageNoADFilterForm').css('display') != "none"){
        var windowHeight = $(window).height();
        var mainTitleHeight = $('.mainTitle').height() + 1;
        var mainHeight =$('#baseSetting').height();
        var height = null;

        if(windowHeight > (mainHeight + mainTitleHeight)){
            height = windowHeight;
        }else{
            height = mainHeight + mainTitleHeight;
        }
        $('#manageNoADFilterForm').css('height',height);
        $('#manageNoADFilterHeightDiv').css('height',height / 2 - 300);
        if(windowHeight < 535){
            $('html').css('overflow-y', 'auto');
        }else{
            $('html').css('overflow-y', 'hidden');
        }
    }
}

/**
 * 重置保存密码选择状态
 */
function resetPasswordSave(settingObj)
{
    var value = settingObj.getLoginAssis();
    if(value == "0"){
        passwordSaveCheckBoxReset();
    }else{
        passwordSaveCheckBoxAct();
    }
}
/**
 * 重保存密码checkbox状态
 */
function passwordSaveCheckBoxReset()
{
    $("#passwordSaveCheckBox").removeClass('checkBox_act');
    $("#passwordSaveCheckBox").addClass('checkBox');
}
/**
 * 激活保存密码checkbox状态
 */
function passwordSaveCheckBoxAct()
{
    $("#passwordSaveCheckBox").removeClass('checkBox');
    $("#passwordSaveCheckBox").addClass('checkBox_act');
}
/**
 * 显示已保存密码网站管理窗口
 */
function showManagePasswordSaveForm(settingObj)
{	
	addPasswordSaveList(settingObj);
    $("#passwordSaveForm").show(); 
}
/**
 * 显示不保存密码网站管理窗口
 */
function showManagePasswordNoSaveForm(settingObj)
{	
    addPasswordNoSaveList(settingObj);
    $("#passwordNoSaveForm").show();  
}
/**
 * 添加已保存密码网站列表
 */
function addPasswordSaveList(settingObj)
{
    var html = '';
    var data = settingObj.getLoginList(1);
	
    if(typeof(data) != "undefined"){
        var loginList = data.split('|||');
        for(var i = 0; i < loginList.length; i++){
			if(loginList[i] != ""){
				var loginListItem = loginList[i].split(',');
				var src = loginListItem[1];
				var icon = settingObj.getUrlIconPath(src);
			    if(typeof(icon) == "undefined") 
					icon = "favicon0.png";
				//html += '<li><img src="' + icon + '" /><span>'+ loginListItem[1] + '</span><span>'+ loginListItem[2] + '</span><span class="delPasswordSaveItem" ><img src="close2.png" /></span></li>';
				html += '<li><h3><img src="' + icon + '" /></h3><h4 num =" ' +loginListItem[0]+ '" title="' + loginListItem[1] + '">' + loginListItem[1] + '</h4><h5 title="' + loginListItem[3] + '">' + loginListItem[3] + '</h5><span class="delPasswordSaveItem" ><img src="close2.png" /></span></li>'
            }
        }
    }
    $("#passwordSaveList").html(html);
 /**
 * 点击关闭已保存密码按钮
 */
    $("#passwordSaveCloseBtn").click(function(){
        hideManagePasswordSaveForm();
    });
	$("#passwordSaveCancleBtn").click(function(){
        hideManagePasswordSaveForm();
    });
	delPasswordRecord(settingObj);
}
/**
 * 添加已保存密码网站列表
 */
function addPasswordNoSaveList(settingObj)
{
    var html = '';
    var data = settingObj.getLoginList(3);
	
    if(typeof(data) != "undefined"){
        var loginList = data.split('|||');
        for(var i = 0; i < loginList.length; i++){
			if(loginList[i] != ""){
				var loginListItem = loginList[i].split(',');
				var src = loginListItem[0];
				var icon = settingObj.getUrlIconPath(src);
				//alert(src + "   " + icon);
			    if(typeof(icon) == "undefined") 
			    	icon = "favicon0.png";
				html += '<li><h3><img src="' + icon + '" /></h3><h4 title="' + loginListItem[0] + '">' + loginListItem[0] + '</h4><span class="delPasswordNoSaveItem" ><img src="close2.png" /></span></li>';

            }
        }
    }
	
    $("#passwordNoSaveList").html(html);
 /**
 * 点击关闭已保存密码按钮
 */
    $("#passwordNoSaveCloseBtn").click(function(){
        hideManagePasswordNoSaveForm();
    });
	$("#passwordNoSaveCancleBtn").click(function(){
        hideManagePasswordNoSaveForm();
    });
	delPasswordNoSaveRecord(settingObj);
}
 /**
 * 点击删除项目按钮
 */
function delPasswordRecord(settingObj){
	$(".delPasswordSaveItem").delegate("img","click",function(){
		//alert($(this).parent().parent().children("h4").attr("num"));
		$(this).parent().parent().hide();
		var num = $(this).parent().parent().children("h4").attr("num");
		settingObj.getLoginList(2,num);
	});
}
 /**
 * 点击不保存网站项目按钮
 */
function delPasswordNoSaveRecord(settingObj){
	$(".delPasswordNoSaveItem").delegate("img","click",function(){
		$(this).parent().parent().hide();
		var url = $(this).parent().parent().children("h4").html();
		settingObj.getLoginList(4,url);
	});
}

