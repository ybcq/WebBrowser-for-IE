/**
 * 重置已保存密码网站管理窗口高度
 */
function resetPasswordSaveFormHeight()
{
   // if($('#passwordSaveForm').css('display') != "none"){
        var windowHeight = $(window).height();
        var mainTitleHeight = $('.mainTitle').height() + 1;
        var mainHeight =$('#baseSetting').height();
        var height = null;

        if(windowHeight > (mainHeight + mainTitleHeight)){
            height = windowHeight;
        }else{
            height = mainHeight + mainTitleHeight;
        }
        $('#passwordSaveForm').css('height',height);
        $('#passwordSaveHeightDiv').css('height',height / 2 - 300);
        if(windowHeight < 535){
            $('html').css('overflow-y', 'auto');
        }else{
            $('html').css('overflow-y', 'hidden');
        }
  //  }
}
