/**
 * 重置左侧菜单高度
 */
function resetLeftMenuHeight()
{
    var windowHeight = $(window).height();
    var mainTitleHeight = $('.mainTitle').height() + 1;
    var height = null;
    var mainHeight = null;
    if($('#baseSetting').css('display') != "none")
    {
        mainHeight = $('#baseSetting').height();
    }
    if($('#tabSetting').css('display') != "none")
    {
        mainHeight = $('#tabSetting').height();
    }
	if($('#downloadSetting').css('display') != "none")
    {
        mainHeight = $('#downloadSetting').height();
    }
    if($('#internetTraces').css('display') != "none")
    {
        mainHeight = $('#internetTraces').height();
    }
    if($('mouseHand').css('display') != "none")
    {
        mainHeight = $('mouseHand').height();
    }
    if(windowHeight > (mainHeight + mainTitleHeight)){
        height = windowHeight - mainTitleHeight;
    }else{
        height = mainHeight;
    }
    $('.leftList').css('height',height);
}

/**
 * 重置用户自定义主页窗口高度
 */
function resetUserDefinedHomePageFormHeight()
{
    if($('#userDefinedHomePageForm').css('display') != "none"){
        var windowHeight = $(window).height();
        var mainTitleHeight = $('.mainTitle').height() + 1;
        var mainHeight =$('#baseSetting').height();
        var height = null;

        if(windowHeight > (mainHeight + mainTitleHeight)){
            height = windowHeight;
        }else{
            height = mainHeight + mainTitleHeight;
        }
        $('#userDefinedHomePageForm').css('height',height);
        $('#heightDiv').css('height',height / 2 - 200);

        if(windowHeight < 280){
            $('html').css('overflow-y', 'auto');
        }else{
            $('html').css('overflow-y', 'hidden');
        }
    }
}

/**
 * 重置右侧选项尺寸
 */
function resetRightOptionSize()
{
    var windowHeight = $(window).height();
    var windowWidth = $(window).width();
    var mainTitleHeight = $('.mainTitle').height() + 1;
    var LeftMenuWidth = $('.leftList').width() + 1;
    var mainHeight =windowHeight - mainTitleHeight;
    var mainWidth =windowWidth - LeftMenuWidth;
    var maxMainHeight = 833;

    if($('#baseSetting').css('display') != "none")
    {
        $('#baseSetting').css('height',mainHeight);
        $('#baseSetting').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#baseSetting').css('overflow-x', 'auto');
        }else{
            $('#baseSetting').css('overflow-x', 'hidden');
        }
    }
    if($('#tabSetting').css('display') != "none")
    {
        $('#tabSetting').css('height',mainHeight);
        $('#tabSetting').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#tabSetting').css('overflow-x', 'auto');
        }else{
            $('#tabSetting').css('overflow-x', 'hidden');
        }
    }
	if($('#downloadSetting').css('display') != "none")
    {
        $('#downloadSetting').css('height',mainHeight);
        $('#downloadSetting').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#downloadSetting').css('overflow-x', 'auto');
        }else{
            $('#downloadSetting').css('overflow-x', 'hidden');
        }
    }
    if($('#internetTraces').css('display') != "none")
    {
        $('#internetTraces').css('height',mainHeight);
        $('#internetTraces').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#internetTraces').css('overflow-x', 'auto');
        }else{
            $('#internetTraces').css('overflow-x', 'hidden');
        }
    }
    if($('#mouseHand').css('display') != "none")
    {
        $('#mouseHand').css('height',mainHeight);
        $('#mouseHand').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#mouseHand').css('overflow-x', 'auto');
        }else{
            $('#mouseHand').css('overflow-x', 'hidden');
        }
    }
	if($('#moreSetting').css('display') != "none")
    {
        $('#moreSetting').css('height',mainHeight);
        $('#moreSetting').css('width',mainWidth);
        if(windowWidth < maxMainHeight){
            $('#moreSetting').css('overflow-x', 'auto');
        }else{
            $('#moreSetting').css('overflow-x', 'hidden');
        }
    }
}

/**
 * 开始清除上网痕迹
 */
function startCleanPrivacy(settingObj)
{
    settingObj.startCleanPrivacy();
    $("#waitingImg").show();
    $("#startCleanPrivacyBtn").hide();
    $("#startCleanPrivacyBtnOff").show();
}

/**
 * 获取是否正在清理
 */
function getCleanPrivacyStatus(settingObj)
{
    var value = settingObj.getCleanPrivacyStatus();
    if(value == "0"){
        $("#waitingImg").hide();
		$("#startCleanPrivacyBtn").show();
		$("#startCleanPrivacyBtnOff").hide();
        clearInterval(cleanPrivacyTimer);
        if($(".savedTip").attr("flag") == "1"){
            showCleanedMessage();
			$("#startCleanPrivacyBtn").show();
   			$("#startCleanPrivacyBtnOff").hide();
        }
    }else{
        $("#waitingImg").show();
        $("#startCleanPrivacyBtn").hide();
    	$("#startCleanPrivacyBtnOff").show();
    }
}

var cleanPrivacyTimer = null;

/**
 * 检测是否正在清理
 */
function checkCleanPrivacyStatus(settingObj)
{
    clearInterval(cleanPrivacyTimer);
    cleanPrivacyTimer = setInterval(function(){
        getCleanPrivacyStatus(settingObj);
    },1000);
}

var bossKeyCustomTimer = null;

/**
 * 老板键自定义按键定时器
 */
function checkBossKeyCustomStatus(settingObj,currentBossKey)
{
    clearInterval(bossKeyCustomTimer);
    bossKeyCustomTimer = setInterval(function(){
        getBossKeyCustomStatus(settingObj, currentBossKey);
    },200);
}

/**
 * 
 */
function getBossKeyCustomStatus(settingObj, currentBossKey)
{
    var bossKeyType = settingObj.getBossKey();
	if(bossKeyType != currentBossKey){
		displayBossKeyOption(bossKeyType);
		clearInterval(bossKeyCustomTimer);
		showSuccessMessage();
	}
}


/**
 * 重置关闭当前标签后，切换到状态
 */
//已替换

/**
 * 重置下载标签后，初始化显示
 */
//已替换
/**
 * 重置下载标签后，初始化显示下载工具列表
 */
function resetDownloadToolsList(settingObj){
		var value = settingObj.GetDownTools();
		var toolname = value.split("|");
		var xunlei=$.inArray("17",toolname);
		var kuaiche=$.inArray("18",toolname);
		var xuanfeng=$.inArray("19",toolname);
		if(xunlei < 0){
			$("#chooseXunlei").hide();
			$("#chooseXunleiOff").show();
		}else{
			$("#chooseXunlei").show();
			$("#chooseXunleiOff").hide();
			}
			
		if(kuaiche < 0){
			$("#chooseKuaiche").hide();
			$("#chooseKuaicheOff").show();
		}else{
			$("#chooseKuaiche").show();
			$("#chooseKuaicheOff").hide();
			}
			
		if(xuanfeng<0){
			$("#chooseXuanfeng").hide();
			$("#chooseXuanfengOff").show();
		}else{
			$("#chooseXuanfeng").show();
			$("#chooseXuanfengOff").hide();
			}
	}

/**
 * 重置在新标签打开收藏夹里的网址状态
 */
function resetDbClickCloseTab(settingObj)
{
    var value = settingObj.getDbClickCloseTab();
    if(value == "1"){
        setDbClickCloseTabCheckBox();
    }else{
        resetDbClickCloseTabCheckBox();
    }
}

/**
 * 重置右击关闭对应标签（按住shift）再右击可显示菜单状态
 */
function resetRbClickCloseTab(settingObj)
{
    var value = settingObj.getRbClickCloseTab();
    if(value == "1"){
        setRbClickCloseTabCheckBox();
    }else{
        resetRbClickCloseTabCheckBox();
    }
}

/**
 * 重置新打开的标签放在状态
 */
//已替换

/**
 * 重置在新标签打开收藏夹里的网址状态
 */
function resetAlwaysActiveNewTab(settingObj)
{
    var value = settingObj.getAlwaysActiveNewTab();
    if(value == "1"){
        setAlwaysActiveNewTabCheckBox();
    }else{
        resetAlwaysActiveNewTabCheckBox();
    }
}

/**
 * 重置在新标签打开收藏夹里的网址状态
 */
function resetNewTabFromBookmark(settingObj)
{
    var value = settingObj.getNewTabFromBookmark();
    if(value == "1"){
        setNewTabFromBookmarkCheckBox();
    }else{
        resetNewTabFromBookmarkCheckBox();
    }
}

/**
 * 重置在新标签打开地址栏输入的网址状态
 */
function resetNewTabFromAddr(settingObj)
{
    var value = settingObj.getNewTabFromAddrBox();
    if(value == "1"){
        setNewTabFromAddrBoxCheckBox();
    }else{
        resetNewTabFromAddrBoxCheckBox();
    }
}

/**
 * 重置自动清除状态
 */
function resetCleanPrivacyOnExit(settingObj)
{
    var value = settingObj.getCleanPrivacyOnExit();
    if(value == "1"){
        setCleanPrivacyOnExitCheckBox();
    }else{
        resetCleanPrivacyOnExitCheckBox();
    }
}

/**
 * 重置清除上网痕迹选项
 */
function resetCleanPrivacyOption(settingObj)
{
    var value = settingObj.getCleanPrivacy();
    if((value & 2) > 0){
        setCleanPrivacyCheckBox(2);
    }
    if((value & 16) > 0){
        setCleanPrivacyCheckBox(16);
    }
    if((value & 8) > 0){
        setCleanPrivacyCheckBox(8);
    }
    if((value & 1) > 0){
        setCleanPrivacyCheckBox(1);
    }
    if((value & 4) > 0){
        setCleanPrivacyCheckBox(4);
    }
}

/**
 * 重置清除上网痕迹时间
