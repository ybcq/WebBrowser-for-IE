	 /**
     * 点击鼠标手势下拉列表
     */
	$("td.MouseGestureDefine div.drawDown").click(function(event){
		event.stopPropagation();
		for(i = 0; i <= 16; i++){
			if($("ul.MouseGestureActList").eq(i).parent("div.drawDown").attr("mgid") != $(this).children("ul.MouseGestureActList").parent(".drawDown").attr("mgid")){
				$("ul.MouseGestureActList").eq(i).hide();
				
			}else{
				$("ul.MouseGestureActList").eq(i).toggle();
			}
		}
	});
		
	/**
     * 鼠标手势下拉列表选项
     */
    $(".MouseGestureActList").delegate("li","click",function(event){
		event.stopPropagation();
		var mgid = $(this).parent().parent().attr("mgid");
		var actid = $(this).attr("doid");
		eval('settingObj.setMouseGestureNum'+ mgid +'(actid);');
		$(this).parent().parent().children("#MouseGestureToDo").text(doarry[actid]);
		showSuccessMessage();
		$(this).parent(".MouseGestureActList").hide();
    });
	

	
	
	/**
     * 选择翻译助手选项
     */
    $("#translateHelperDrawDownList").delegate("li","click",function(){
        var optionID = $(this).attr("id");
        if(optionID == "showTransBarAndIcon"){
			settingObj.setTranslateMode("0");
			$("#transPop").show();
			$("#transPopHide").hide();
			showSuccessMessage();
        }
        if(optionID == "showTransIcon"){
 			settingObj.setTranslateMode("1");
			$("#transPop").show();
			$("#transPopHide").hide();
			showSuccessMessage();
        }
        if(optionID == "showTransNo"){
			settingObj.setTranslateMode("2");
			$("#transPop").hide();
			$("#transPopHide").show();
			showSuccessMessage();
        }
    });
	/**
