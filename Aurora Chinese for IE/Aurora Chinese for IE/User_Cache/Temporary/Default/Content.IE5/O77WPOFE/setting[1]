//已替换
/**
 * 重置主页选择框列表自定义部分
 */
function homePageUserDefinedOptionReset(settingObj)
{
    var url = settingObj.getHomePage();
    var urlList = url.split('\r\n');
    url = urlList[0];
    var text = url;    
	//if(text.length > 12){
//        text = text.substring(0, 12) + "...";
//    }
    if(text == ""){
        $("#homePageUserDefinedOption").hide();
    }else{
        $("#homePageUserDefinedOption").show();
        $("#homePageUserDefinedOptionText").html(filterStr(text));

        var src = settingObj.getUrlIconPath(url);
        if(typeof(src) == "undefined"){
            src = "favicon0.png"
        }
        $("#homePageUserDefinedOptionIco").attr("src", filterUrl(src));
    }
}

/**
 * 重置主页icon
 */
function homePageIconReset()
{
    $("#hao123Option").children("span").children("img").attr("src","favicon1.png");
    $("#baiduOption").children("span").children("img").attr("src","favicon2.png");
    $("#newPageOption").children("span").children("img").attr("src","favicon3.png");
	$("#lastOpenPageOption").children("span").children("img").attr("src","favicon4.png");
}

