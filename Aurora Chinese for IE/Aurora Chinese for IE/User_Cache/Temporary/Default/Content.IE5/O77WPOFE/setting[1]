/**
 * 设置清除上网痕迹选项
 */
function setCleanPrivacyOption(settingObj)
{
    var value = 0;
    var classname = $("#cleanPrivacyCheckBox2").attr("class");
    if(classname == "checkBox_act"){
        value = value | 2;
    }
    classname = $("#cleanPrivacyCheckBox16").attr("class");
    if(classname == "checkBox_act"){
        value = value | 16;
    }
    classname = $("#cleanPrivacyCheckBox8").attr("class");
    if(classname == "checkBox_act"){
        value = value | 8;
    }
    classname = $("#cleanPrivacyCheckBox1").attr("class");
    if(classname == "checkBox_act"){
        value = value | 1;
    }
    classname = $("#cleanPrivacyCheckBox4").attr("class");
    if(classname == "checkBox_act"){
        value = value | 4;
    }
    value = value.toString();
    settingObj.setCleanPrivacy(value);
}

/**
 * 重置清除上网痕迹状态
 */
function resetCleanPrivacy(settingObj)
{
    resetCleanPrivacyOption(settingObj);
    resetCleanPrivacyTime(settingObj);
}

/**
 * 重置启动时打开选择状态
 */
function resetStartPageType(settingObj)
{
    hao123CheckRadioReset();
    lastCloseCheckRadioReset();
    homePageCheckRadioReset();
    homePageUserDefinedOptionReset(settingObj);
    currentHomePageSelectReset(settingObj);
    homePageIconReset(settingObj);

    var value = settingObj.getStartPageType();
    if(value == "0"){
        hao123CheckRadioAct();
    }
    if(value == "1"){
        lastCloseCheckRadioAct();
    }
    if(value == "2"){
        homePageCheckRadioAct();
    }
}

/**
 * 重置关闭浏览器时状态
 */
function resetConfirmCloseAll(settingObj)
{
    var value = settingObj.getCloseMultiTabTip();
    if(value == 1){
        multiTabAskCloseCheckBoxAct();
    }else{
        multiTabAskCloseCheckBoxReset();
    }
}
/**
 * 重置选用桔子浏览器下载时保存复选框状态
 */
function resetDownloadSaveLocation(settingObj)
{
    var value = settingObj.getAskSaveLocation();
	//var value = 0;
    if(value == 1){
        resetDownloadSaveLocationAct();
    }else{
        resetDownloadSaveLocationReset();
    }
}
/**
 * 重置选用桔子浏览器下载时显示列表复选框状态
 */
function resetDownloadListWindow(settingObj)
{
    var value = settingObj.getShowDownlistWnd();
	//var value = 0;
    if(value == 1){
        resetDownloadListWindowAct();
    }else{
        resetDownloadListWindowReset();
    }
}


/**
 * 开启鼠标手势状态
 */
function resetMouseGesture(settingObj)
{
    var value = settingObj.getMouseGesture();
    if(value == 1){
        mouseGestureCheckBoxAct();
    }else{
        mouseGestureCheckBoxReset();
    }
}

/**
 * 重置广告过滤选择状态
 */
function resetADFilterClass(settingObj)
{
    noADFilterCheckRadioReset();
    aotoADFilterCheckRadioReset();
    aotoAndFloatADFilterCheckRadioReset();

    var value = settingObj.getADFilterClass();
    if(value == "0"){
        noADFilterCheckRadioAct();
    }
    if(value == "1"){
        aotoADFilterCheckRadioAct();
    }
    if(value == "2"){
        aotoAndFloatADFilterCheckRadioAct();
    }
}

/**
 * 重置弹出广告选择状态
 */
function resetWinBlock(settingObj)
{
    var value = settingObj.getWinBlock();
    if(value == "0"){
        winBlockCheckBoxReset();
    }else{
        winBlockCheckBoxAct();
    }
}

/**
 * 重置页面广告选择状态
 */
function resetAdBlock(settingObj)
{
    var value = settingObj.getAdBlock();
    if(value == "0"){
        adBlockCheckBoxReset();
    }else{
        adBlockCheckBoxAct();
    }
}
