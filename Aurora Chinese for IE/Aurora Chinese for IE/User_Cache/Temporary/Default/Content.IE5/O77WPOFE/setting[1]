/**
 * 重置buscape shopping helper状态
 */
function resetShoppingHelper(settingObj)
{
    var value = settingObj.getShopHelperSetting();
    if(value == "0"){
        shoppingHelperCheckBoxReset();
    }else{
        shoppingHelperCheckBoxBoxAct();
    }
}
/**
 * buscape shopping helper checkbox状态
 */
function shoppingHelperCheckBoxReset()
{
    $("#shoppingHelperCheckBox").removeClass('checkBox_act');
    $("#shoppingHelperCheckBox").addClass('checkBox');
}
/**
 * 激活buscape shopping helper checkbox状态
 */
function shoppingHelperCheckBoxBoxAct()
{
    $("#shoppingHelperCheckBox").removeClass('checkBox');
    $("#shoppingHelperCheckBox").addClass('checkBox_act');
}

/**
 * 重置aurora translate helper状态
 */
function resetTranslateHelper(settingObj)
{
    var value = settingObj.getTranslateHelperMode();
    if(value == "0"){
        translateHelperCheckBoxReset();
    }else{
        translateHelperCheckBoxBoxAct();
    }
}
/**
 * aurora translate helper checkbox状态
 */
function translateHelperCheckBoxReset()
{
    $("#translateHelperCheckBox").removeClass('checkBox_act');
    $("#translateHelperCheckBox").addClass('checkBox');
}
/**
 * 激活aurora translate helper checkbox状态
 */
function translateHelperCheckBoxBoxAct()
{
    $("#translateHelperCheckBox").removeClass('checkBox');
    $("#translateHelperCheckBox").addClass('checkBox_act');
}


