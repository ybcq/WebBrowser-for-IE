/*
* 更改语言
*/
function changeLanguage(settingObj){
	var cl = settingObj.getProcessLanguage();
	if(cl == 1){
		$("#currentLanguage").html("English");
		$("#currentLanguageOptionText").html("&nbsp;&nbsp;English");
		}
//	else if(cl == 2){
//		$("#currentLanguage").html("中文");
//		$("#currentLanguageOptionText").html("&nbsp;&nbsp;中文");
//		}
	else if(cl == 3){
		$("#currentLanguage").html("Português");
		$("#currentLanguageOptionText").html("&nbsp;&nbsp;Português");
		}
}

function initTextLength(){
	if(checkstr($("#defaultBrow").html()) >= 17){
		$("#defaultBrow").attr("class","btnSetDefault2");
		$("#defaultBrowOff").attr("class","btnSetDefault2_un");
	}else{
		$("#defaultBrow").attr("class","btnSetDefault");
		$("#defaultBrowOff").attr("class","btnSetDefault_un");
	}
	if(checkstr($("#newTabAtText").html()) >= 25 || checkstr($("#currentTabR span").html()) >= 25 || checkstr($("#AllTabR span").html()) >= 25){
		$("#newTabAtDrawDown").attr("class","drawDown2");
		$("#newTabAtDrawDownList").attr("class","drawDownList2");
	}else{
		$("#newTabAtDrawDown").attr("class","drawDown");
		$("#newTabAtDrawDownList").attr("class","drawDownList");
	}
	if(checkstr($("#tabClosePolicyText").html()) >= 29 || checkstr($("#tabClosePolicyS span").html()) >= 29 || checkstr($("#tabClosePolicyL span").html()) >= 29 || checkstr($("#tabClosePolicyR span").html()) >= 29){
		$("#tabClosePolicyDrawDown").attr("class","drawDown2");
		$("#tabClosePolicyDrawDownList").attr("class","drawDownList2");
	}else{
		$("#tabClosePolicyDrawDown").attr("class","drawDown");
		$("#tabClosePolicyDrawDownList").attr("class","drawDownList");
	}
	if(checkstr($("#downloadPolicyText").html()) >= 29 || checkstr($("#chooseAurora span").html()) >= 29 || checkstr($("#chooseEverTime span").html()) >= 29 || checkstr($("#chooseXunlei span").html()) >= 29 || checkstr($("#chooseXunleiOff span").html()) >= 29 || checkstr($("#chooseKuaiche span").html()) >= 29 || checkstr($("#chooseKuaicheOff span").html()) >= 29 || checkstr($("#chooseXuanfeng span").html()) >= 29 || checkstr($("#chooseXuanfengOff span").html()) >= 29){
		$("#downloadAtDrawDown").attr("class","drawDown2");
		$("#downloadAtDrawDownList").attr("class","drawDownList2");
	}else{
		$("#downloadAtDrawDown").attr("class","drawDown");
		$("#downloadAtDrawDownList").attr("class","drawDownList");
	}
}

function checkstr(str){      
	var n=0;          
	//var str=document.getElementById('Title').value;
	for(i=0;i<str.length;i++){
		var leg = str.charCodeAt(i);
		if(leg > 255){
			n+=2;
		}else {
			n+=1;
		}
	}
	return n;
}
/**
 * 设置语言
 */
function setLanguageTypeEN(settingObj)
{
	settingObj.setProcessLanguage("1");
	showSuccessMessage();
}

function setLanguageTypeCN(settingObj)
{
	settingObj.setProcessLanguage("2");
	showSuccessMessage();
}

function setLanguageTypePU(settingObj)
{
	settingObj.setProcessLanguage("3");
	showSuccessMessage();
}