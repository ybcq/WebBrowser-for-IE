/**
 * 初始化设置选项
 */
function initSetting(settingObj)
{	
	changeLanguage(settingObj);
	initTextLength();
	resetLeftMenuHeight();
	resetRightOptionSize();
	baseSettingInit(settingObj);
	tabSettingInit(settingObj);
	downloadSettingInit(settingObj);
	internetTracesInit(settingObj);
	mouseGestureSettingInit(settingObj);
	shoppingHelperInit(settingObj);
	translateHelperInit(settingObj);

	whereIn();

	resetPasswordSaveFormHeight();
	resetPasswordNoSaveFormHeight();

    hideTranslater(settingObj);
}
function hideTranslater(settingObj){
    var t = settingObj.gettranslateShow();
    if(t==1){
        $(".translater").show();
    }
    if(t==0){
        $(".translater").hide();
    }
}
/**
 * 默认进入选项
 */
function whereIn()
{
    // 参数判断
    var p = window.location.hash;
    if(p == '#b'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("baseSettingMenu");
        setCurrentSettingMain("baseSetting");
        resetRightOptionSize();
    }
    if(p == '#t'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("tabSettingMenu");
        setCurrentSettingMain("tabSetting");
        resetRightOptionSize();
    }
	if(p == '#d'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("downloadSettingMenu");
        setCurrentSettingMain("downloadSetting");
        resetRightOptionSize();
    }
    if(p == '#c'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("internetTracesMenu");
        setCurrentSettingMain("internetTraces");
        resetRightOptionSize();
    }
    if(p == '#m'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("mouseHandMenu");
        setCurrentSettingMain("mouseHand");
        resetRightOptionSize();
    }
 	if(p == '#e'){
        resetLeftMenu();
        resetSettingMain();
        setCurrentMenu("moreMenu");
        setCurrentSettingMain("moreSetting");
        resetRightOptionSize();
    }}

/**
 * 基本设置选项初始化
