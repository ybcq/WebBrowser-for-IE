	};
	MouseGestureListAdd()
}


/**
 * 显示自动翻译的网站管理窗口
 */
function showManageAutoTransForm(settingObj)
{
   addAutoTransList(settingObj);
    $("#manageAutoTransForm").show();
    resetManageAutoTransFormHeight();
}

/**
 * 显示不提示翻译的网站管理窗口
 */
function showManageNoTransForm(settingObj)
{
   	addNoTransList(settingObj);
    $("#manageNoTransForm").show();
    resetManageNoTransFormHeight();
}
/**
 * 添加自动翻译的网站列表
 */
function addAutoTransList(settingObj)
{
    var html = '';
    var data = settingObj.translateSetting(1,"");
    if(typeof(data) != "undefined"){
        var autoTransList = data.split('|||');
        for(var i = 0; i < autoTransList.length; i++){
            if(autoTransList[i] != ""){
                html += '<li><h3>' + autoTransList[i] + '</h3><span ><img src="close2.png" class="delAutoTransItem" /></span></li>';
            }
        }
    }
    $("#manageAutoTransList").html(html);
}
/**
 * 添加不翻译的网站列表
 */
function addNoTransList(settingObj)
{
	var html = '';
	var data = settingObj.translateSetting(4,"");
    if(typeof(data) != "undefined"){
        var noTransList = data.split('|||');
		
        for(var i = 0; i < noTransList.length; i++){
            if(noTransList[i] != ""){
                html += '<li><h3>' + noTransList[i] + '</h3><span ><img src="close2.png" class="delNoTransItem" /></span></li>';
            }
        }
    }
    $("#manageNoTransList").html(html);
}

/**
 * 重置自动翻译的网站管理窗口高度
 */
function resetManageAutoTransFormHeight()
{
    if($('#manageAutoTransForm').css('display') != "none"){
        var windowHeight = $(window).height();
        var mainTitleHeight = $('.mainTitle').height() + 1;
        var mainHeight =$('#baseSetting').height();
        var height = null;

        if(windowHeight > (mainHeight + mainTitleHeight)){
            height = windowHeight;
        }else{
            height = mainHeight + mainTitleHeight;
        }
        $('#manageAutoTransForm').css('height',height);
        $('#manageAutoTransHeightDiv').css('height',height / 2 - 300);
        if(windowHeight < 535){
            $('html').css('overflow-y', 'auto');
        }else{
            $('html').css('overflow-y', 'hidden');
        }
    }
}
